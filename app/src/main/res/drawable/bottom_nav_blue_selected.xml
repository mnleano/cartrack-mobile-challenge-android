<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="-2dp" android:left="-2dp" android:bottom="-1dp">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="1dp"
                android:color="#C9D1DA" />
        </shape>
    </item>
    <item android:right="-4dp" android:left="-4dp" android:top="-4dp">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="3dp"
                android:color="@color/blue" />
        </shape>
    </item>
</layer-list>